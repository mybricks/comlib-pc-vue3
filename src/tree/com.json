{
  "title": "树",
  "namespace": "your-team.lib.tree",
  "version": "1.0.0",
  "description": "树",
  "author": "MyBricks team",
  "author_name": "板砖团队",
  "data": "./data.json",
  "icon":"./icon.svg",
  "runtime": "./runtime.vue",
  "editors": "./editors.tsx",
  "inputs": [
    {
      "id": "treeData",
      "title": "输入数据",
      "desc": "树组件的节点数据，确保key唯一",
      "schema": {
        "title": "树组件数据",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "title": "标题",
              "description": "树节点标题",
              "type": "string"
            },
            "key": {
              "title": "字段名",
              "description": "树节点唯一的key",
              "type": "string"
            },
            "icon": {
              "title": "前置图标",
              "description": "树节点前置的图标",
              "type": "string"
            },
            "children": {
              "title": "子项",
              "type": "array",
              "description": "节点子项的数据",
              "items": {
                "type": "object"
              }
            }
          }
        }
      }
    }
  ],
  "outputs": [
    {
      "id": "click",
      "title": "节点选中事件",
      "desc": "节点选中后，事件输出",
      "schema": {
        "type": "array",
        "description": "所有选中的节点的key",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "onChange",
      "title": "数据变化",
      "desc": "更改树的数据出，触发数据变化输出",
      "schema": {
        "title": "树组件数据",
        "type": "array",
        "description": "树组件的数据",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "title": "标题",
              "type": "string",
              "description": "节点的标题"
            },
            "key": {
              "title": "字段名",
              "type": "string",
              "description": "节点的字段名"

            },
            "children": {
              "title": "子项",
              "type": "array",
              "description": "节点的子项",
              "items": {
                "type": "object"
              }
            }
          }
        }
      }
    }
  ]
}
