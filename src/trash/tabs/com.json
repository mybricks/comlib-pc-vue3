{
  "title": "Tabs",
  "enable": true,
  "visibility": true,
  "namespace": "mybricks.normal-pc.tabs-vue",
  "version": "1.0.00",
  "description": "tabs组件",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.vue",
  "editors": "./editors.tsx",
  "inputs": [
    {
      "id": "dataSource",
      "title": "设置数据源",
      "desc": "设置列表容器数据源，数据结构要求是数组",
      "schema": {
        "title": "列表数据",
        "type": "array",
        "description": "列表容器的数据源，数组格式",
        "items": {
          "title": "列项数据",
          "type": "any"
        }
      },
      "rels": [
        "setDataSourceDone"
      ]
    }
  ],
  "outputs": [
  ],
  "slots": [
    {
      "id": "item",
      "capacity": 1,
      "title": "列表项",
      "description": "列表项作用域插槽，有对应项内容和序号的输入",
      "type": "scope",
      "inputs": [
        {
          "id": "itemData",
          "title": "当前项",
          "desc": "当前项展示内容",
          "schema": {
            "type": "any"
          }
        },
        {
          "id": "index",
          "title": "当前项序号",
          "desc": "当前项展示的序号",
          "schema": {
            "type": "number"
          }
        }
      ]
    },
    {
      "id": "tabId1",
      "capacity": 1,
      "title": "标签项1"
    },
    {
      "id": "tabId2",
      "capacity": 1,
      "title": "标签项2"
    }
  ]
}